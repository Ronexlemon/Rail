datasource db{
        provider = "postgres"
        url = env("DATABASE_URL")
}

generator db{
    provider = "go run github.com/steebchen/prisma-client-go"
}



model Client{
    id    String @id @default(cuid())
    customer_id String @unique
    business_id  String
    created_at DateTime @default(now())
    updated_at DateTime @updatedAt
   
}

