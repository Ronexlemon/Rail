generator db {
  provider = "go run github.com/steebchen/prisma-client-go"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model User {
  id         String   @id @default(uuid())
  
  passwordHash  String
  role  Role 
  email      String   @unique
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}


enum Role {
    ADMIN
    BUSINESS
}


